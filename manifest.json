{
    "manifest_version": 3,
    "name": "Virtual clothes Try-on",
    "version": "1.0",
    "description": "Lets you try clothes virtually",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["<all_urls>"],
    "action": {
        "default_title": "Virtual clothes Try-on"
    },
    "background": {
        "service_worker": "background.js"
    }
}